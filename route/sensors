const express= require('express');
const { serialize } = require('v8');

const router = express.Router(); 

const sensorData= {
    serial:[],
    swVersion:[],
    temperature:[],
    date:[],
    gps:[],

};


router.post('/api/sensors',(req, res, next)=>{
    sensorData.serial.push(req.body.serial);
    sensorData.swVersion.push(req.body.swVersion);
    sensorData.temperature.push(req.body.temperature);
    sensorData.date.push(req.body.date);
    sensorData.gps.push(req.body.gps);
    res.send({message: 'data recieved !', data: sensorData});

});

module.exports = router;